{
  "manifest_version": 3,
  "name": "browser-gpt",
  "version": "0.1.0",
  "action": {
    "default_popup": "index.html"
  },
  "side_panel": {
    "default_path": "index.html",
    "open_icon": "icon.png",
    "close_icon": "icon.png",
    "always_on_top": true
  },
  "description": "Digital Assistance powered by Langchain",
  "permissions": [
    "activeTab",
    "tabs",
    "sidePanel",
    "webNavigation"
  ],
  "host_permissions": [
    "<all_urls>"
    
  ]
}